package Arrays2;

import java.util.Scanner;

public class BinarySearch {

	public static int[] input() {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int arr[] = new int[n];
		for(int i = 0 ; i < n ; i++) {
		 arr[i] = sc.nextInt();
		}
	}
	
	public static binarySearch(int [] arr) {
		int start= 0 ;
		int end = arr.length;
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		
		while( start <= end) {
			int mid = (start + end)/2;
			if( arr[mid] < num  ) {
			start = mid + 1;
				end = end + 0;
				
			}else if( arr[mid] > num) {
				start =  mid - 1;
				end = start;
	 		}else if ( arr[mid] == num) {
	 			return num;
	 		}
		
		
			
		}
	      return -1;
	}

	public static void main(String[] args) {
		

	}

}
