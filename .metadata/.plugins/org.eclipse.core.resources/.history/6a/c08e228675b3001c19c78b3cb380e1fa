package Stacks;

import LinkedList1.Node;

public class StackUsingLinkedList<T> {

    private Node<T> head;
    
    private int size;
    
  public StackUsingLinkedList() {
	   
	   head = null;
	   
	   size = 0;
   }
  
    public void push(T elem) {
    	
     head = new Node<>(elem);
    
     head.next = head;
	 
	 head = head.next;
     
     size+= 1;
    
    }
      
   public int size() {
	  
	  return size;
  
  }
      
//    public boolean isEmpty() {
//    	
//    	
//    
//    }
  
      public T top() {
    	  
      return head.data;	  
      
      }

    
}
