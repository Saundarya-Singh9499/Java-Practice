package Stacks;

public class StackUsingArray {
	
	private int data[];
	private int topIndex;

	public StackUsingArray() {
		
		data = new int[10];
		topIndex = -1;
	}

    public void push(int elem) {
    	
    	data[0] = elem;
    	
    	System.out.println(data[0]);
    }
        public int size() {
        	
        	int count = 0;
        	
        	for(int i = 0 ; i < data.length ; i++) {
        		
        		count+= 1;
        	}
              return count;
        }
 
//     public int top() {
//    	 
//    	 
//     }
}
