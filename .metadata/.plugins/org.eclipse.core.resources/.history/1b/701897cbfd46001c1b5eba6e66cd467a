package OperatorsandforLoops;

public class IncreamentDecreament {

	public static void main(String[] args) {
		
		int a =40;
		int b=60;
		
       boolean bl;
		if(a++ >10 && b++ >100) {
        	bl = true;
        	}
        else {
        	bl= false;
        }
		System.out.println(a + " " + b);

	}

}
