package LinkedList1;

import java.util.ArrayList;

public class ArrayListDemo {

	public static void main(String[] args) {
		
		ArrayList<Integer> arr = new ArrayList<>(20);

	    arr.add(10);
	    arr.add(20);
	    arr.add(30);
	    arr.add(40);
	    
	    arr.remove(0);
	    arr.remove(2)
	
	
	
	}

}
