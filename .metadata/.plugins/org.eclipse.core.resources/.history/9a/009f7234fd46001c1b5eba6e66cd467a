package OperatorsandforLoops;

public class IncreamentDecreament {

	public static void main(String[] args) {
		
		int a =40;
		int b=60;
		
        if(++a>50 && b++ >100) {
        	System.out.println("In If");
        	}
        else {
        	System.out.println("In Else");
        }
		System.out.println(a + " " + b);

	}

}
