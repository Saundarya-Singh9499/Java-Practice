package Stacks;

public class StackUsingArray {
	
	private int data[];
	private int topIndex;

	public StackUsingArray() {
		
		data = new int[10];
		topIndex = -1;
	}

    public void push(int elem) {
    	
    	data[0] = elem;
    	topIndex+= 1;
    	
     }
        public int size() {
        	
         return topIndex + 1;
        }
     public void top() {
    	 
     	 System.out.println(data[0]);
     
     }
}
