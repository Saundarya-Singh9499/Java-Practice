package OperatorsandforLoops;

public class IncreamentDecreament {

	public static void main(String[] args) {
		
		int a =40;
		int b=60;
		
       boolean bl;
		if(a++ >100 && b++ >100) {
        	System.out.println(bl = true);
        	}
        else {
        	System.out.println(bl = false);
        }
		System.out.println(a + " " + b);

	}

}
